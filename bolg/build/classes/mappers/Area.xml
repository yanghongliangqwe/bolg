<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="com.company.dao.IAreaDao">
  <select id="findAreasByCid" resultType="com.company.bean.Area">
  select id,areaID,areas,cityID from area where cityID =#{cityID}
  </select>
  <select id="findObject" resultType="com.company.bean.Area">
  select id,areaID,areas,cityID from area where areaID = #{areaID}
  </select>
  </mapper>